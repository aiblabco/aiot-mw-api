<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>AIoTMW</title>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" />
  <link href="https://cdn.datatables.net/1.13.6/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
  <link href="https://cdn.datatables.net/responsive/2.5.0/css/responsive.dataTables.min.css" rel="stylesheet" />
  <link
    rel="stylesheet"
    href="https://cdnjs.cloudflare.com/ajax/libs/monaco-editor/0.41.0/min/vs/editor/editor.main.min.css"
    integrity="sha512-MFDhxgOYIqLdcYTXw7en/n5BshKoduTitYmX8TkQ+iJOGjrWusRi8+KmfZOrgaDrCjZSotH2d1U1e/Z1KT6nWw=="
    crossorigin="anonymous"
    referrerpolicy="no-referrer"
  />
  <script
    src="https://cdnjs.cloudflare.com/ajax/libs/echarts/5.4.3/echarts.min.js"
    integrity="sha512-EmNxF3E6bM0Xg1zvmkeYD3HDBeGxtsG92IxFt1myNZhXdCav9MzvuH/zNMBU1DmIPN6njrhX1VTbqdJxQ2wHDg=="
    crossorigin="anonymous"
    referrerpolicy="no-referrer"
  ></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.29.4/moment.min.js"></script>

  <style>
    body {
      background-color: #f0f0f0;
    }
    .container {
      background-color: white;
      padding: 25px 30px;
    }

    .editor_area {
      height: 400px;
      border: 1px solid #dfdfdf;
      border-radius: 3px;
      overflow: hidden;
    }

    @media (min-height: 850px) {
      .editor_area {
        height: 600px;
      }
    }

    @media (min-height: 1100px) {
      .editor_area {
        height: 800px;
      }
    }

    .chart_area {
      overflow: hidden;
      width: 100%;
      height: 400px;
    }

    @media (min-height: 850px) {
      .chart_area {
        height: 600px;
      }
    }

    @media (min-height: 1100px) {
      .chart_area {
        height: 800px;
      }
    }

    @media (min-width: 1400px) {
      .container,
      .container-lg,
      .container-md,
      .container-sm,
      .container-xl {
        max-width: 1300px;
      }
    }
    @media (min-width: 1600px) {
      .container,
      .container-lg,
      .container-md,
      .container-sm,
      .container-xl {
        max-width: 1500px;
      }
    }
  </style>
</head>
